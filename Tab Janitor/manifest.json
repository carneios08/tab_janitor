{
    "name": "Tab Janitor",
    "version": "1.0",
    "description": "A tab manager that removes redundant URL navigation in Chrome.",
    "permissions": [
        "storage",
        "tabs",
        "webNavigation"
    ],
    "options_page": "options.html",
    "background": {
        "scripts": [
            "/src/main.js"
        ],
        "persistent": false
    },
    "manifest_version": 2
}