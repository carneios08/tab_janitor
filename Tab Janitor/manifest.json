{
    "name": "Tab Janitor",
    "version": "1.0",
    "description": "Removes tab clutter by collapsing an opening tab into a similar tab that is already opened.",
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab",
        "tabs"
    ],
    "options_page": "options.html",
    "background": {
        "scripts": [
            "/src/main.js"
        ],
        "persistent": false
    },
    "manifest_version": 2
}